{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DATABASE_URL": "@database_url",
    "SECRET_KEY": "@secret_key",
    "HUGGINGFACE_TOKEN": "@huggingface_token"
  },
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "regions": ["iad1"]
}